<!DOCTYPE html>
<html>
<head>
  <title>Mi Portafolio</title>
  <link rel="stylesheet" href="/css/style.css">
</head>
<body>
  <h1>Mis Proyectos</h1>
  <ul>
    <% proyectos.forEach(proyecto => { %>
      <li>
        <h2><a href="/proyecto/<%= proyecto._id %>"><%= proyecto.nombre %></a></h2>
        <p><%= proyecto.descripcion %></p>
      </li>
    <% }); %>
  </ul>
</body>
</html>
